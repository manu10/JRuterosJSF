<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">

<navigation-rule>
<from-view-id>/registration.xhtml</from-view-id>
<navigation-case>
<from-outcome>exito</from-outcome>
<to-view-id>/index.xhtml</to-view-id>
</navigation-case>
</navigation-rule>
<navigation-rule>
	<display-name>editUser.xhtml</display-name>
	<from-view-id>/editUser.xhtml</from-view-id>
	<navigation-case>
		<from-outcome>exito</from-outcome>
		<to-view-id>/index.xhtml</to-view-id>
	</navigation-case>
</navigation-rule>
<navigation-rule>
	<display-name>WEB-INF/facelets/templates/header.xhtml</display-name>
	<from-view-id>/WEB-INF/facelets/templates/header.xhtml</from-view-id>
	<navigation-case>
		<from-outcome>showUser</from-outcome>
		<to-view-id>/users.xhtml</to-view-id>
	</navigation-case>
</navigation-rule>
<navigation-rule>
	<display-name>login.xhtml</display-name>
	<from-view-id>/login.xhtml</from-view-id>
	<navigation-case>
		<from-outcome>successfulLogin</from-outcome>
		<to-view-id>/index.xhtml</to-view-id>
	</navigation-case>
</navigation-rule>
<navigation-rule>
	<display-name>users.xhtml</display-name>
	<from-view-id>/users.xhtml</from-view-id>
	<navigation-case>
		<from-outcome>deshabilitado</from-outcome>
		<to-view-id>/users.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
		<from-outcome>habilitado</from-outcome>
		<to-view-id>/users.xhtml</to-view-id>
	</navigation-case>
</navigation-rule>
<navigation-rule>
	<display-name>login.xhtml</display-name>
	<from-view-id>/login.xhtml</from-view-id>
	<navigation-case>
		<from-outcome>unsuccessfulLogin</from-outcome>
		<to-view-id>/login.xhtml</to-view-id>
	</navigation-case>
</navigation-rule>

</faces-config>
